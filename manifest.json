 {
    "name": "Meeting Goals",
    "version": "1.0",
    "description": "Organize better meetings!",
    "permissions": ["declarativeContent", "storage", "activeTab", "tabs", "webNavigation"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },

    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/CheckBox16.png",
        "32": "images/CheckBox32.png",
        "48": "images/CheckBox48.png",
        "128": "images/CheckBox128.png"
      }
      //page action for inserting HTML
  },
    "content_scripts": [{
      "matches": ["https://calendar.google.com/calendar/*"],
        "js":["background.js"],
        "run_at": "document_idle"
    }],

    
    "icons": {
      "16": "images/CheckBox16.png",
      "32": "images/CheckBox32.png",
      "48": "images/CheckBox48.png",
      "128": "images/CheckBox128.png"
    },
    "manifest_version": 2
  }